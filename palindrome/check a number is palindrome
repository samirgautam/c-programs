#include<stdio.h>
#include<conio.h>
void main()
{
    int n,num,rem,sum=0;
    printf("enter a number:");
    scanf("%d",&n);
    num=n;
    while(n!=0)
    {
        rem=n%10;
        sum=sum*10+rem;
        n=n/10;
    }
    if(num==sum)
    {
        printf("%d is palindrome.",num);
    }
    else
    {
        printf("%d is not a palindrome.",num);
    }
    getch();
}
